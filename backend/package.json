{
  "name": "backend",
  "version": "1.0.0",
  "description": "Fastify backend with PostgreSQL Master-Replica",
  "type": "module",
  "main": "dist/index.js",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "dotenv -e ../.env -- prisma generate",
    "prisma:migrate": "dotenv -e ../.env -- prisma migrate dev",
    "prisma:migrate:deploy": "dotenv -e ../.env -- prisma migrate deploy",
    "prisma:migrate:reset": "dotenv -e ../.env -- prisma migrate reset",
    "prisma:studio": "dotenv -e ../.env -- prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "migrate": "dotenv -e ../.env -- prisma migrate dev",
    "migrate:deploy": "dotenv -e ../.env -- prisma migrate deploy",
    "migrate:reset": "dotenv -e ../.env -- prisma migrate reset",
    "seed": "dotenv -e ../.env -- prisma db seed",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "zod": "^3.23.8",
    "pg": "^8.11.3",
    "redis": "^4.7.0",
    "ioredis": "^5.3.2",
    "minio": "^8.0.1"
  },
  "devDependencies": {
    "prisma": "^6.1.0",
    "dotenv-cli": "^7.4.2",
    "typescript": "^5.3.3",
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.0",
    "tsx": "^4.7.1",
    "nodemon": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "eslint-config-prettier": "^9.1.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}