{
  "name": "backend",
  "version": "1.0.0",
  "description": "Fastify backend with PostgreSQL Master-Replica",
  "type": "module",
  "main": "dist/index.js",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "dotenv -e ../.env -- prisma generate",
    "prisma:migrate": "dotenv -e ../.env -- prisma migrate dev",
    "prisma:migrate:deploy": "dotenv -e ../.env -- prisma migrate deploy",
    "prisma:migrate:reset": "dotenv -e ../.env -- prisma migrate reset",
    "prisma:studio": "dotenv -e ../.env -- prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "migrate": "dotenv -e ../.env -- prisma migrate dev",
    "migrate:deploy": "dotenv -e ../.env -- prisma migrate deploy",
    "migrate:reset": "dotenv -e ../.env -- prisma migrate reset",
    "seed": "dotenv -e ../.env -- prisma db seed",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@prisma/client": "^6.1.0",
    "prisma": "^6.1.0",
    "dotenv-cli": "^7.4.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}